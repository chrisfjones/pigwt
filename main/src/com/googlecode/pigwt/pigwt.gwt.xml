<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE module PUBLIC '-//Google Inc.//DTD Google Web Toolkit 2.0//EN'
        'http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd'>

<module rename-to="app">
    <inherits name='com.google.gwt.user.User'/>
    <!--<inherits name='com.google.gwt.inject.Inject'/>-->

    <entry-point class='com.googlecode.pigwt.client.EntryPoint'/>

    <define-configuration-property name="injector" is-multi-valued="false" />

    <set-property name='user.agent' value='gecko1_8'/>

    <generate-with class="com.googlecode.pigwt.rebind.PageTreeGenerator">
      <when-type-assignable class="com.googlecode.pigwt.client.PageTree" />
    </generate-with>

    <source path="client"/>
</module>
