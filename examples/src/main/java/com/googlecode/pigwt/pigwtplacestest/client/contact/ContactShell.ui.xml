<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <table border="1">
            <tr>
                <td colspan="3"><h3>Contact Detail</h3></td>
            </tr>
            <tr>
                <td>
                    <g:Label ui:field="demographic">Demographic</g:Label>
                </td>
                <td>
                    <g:Label ui:field="addresses">Addresses</g:Label>
                </td>
                <td>
                    <g:Label ui:field="other">Other</g:Label>
                </td>
            </tr>
            <tr>
                <td colspan="3" style="background-color: lightblue;">
                    <g:SimplePanel ui:field="main"></g:SimplePanel>
                </td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>
